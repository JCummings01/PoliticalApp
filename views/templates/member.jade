div.well
  img(src="https://s3.amazonaws.com/assets.opensecrets.org/politicians/img/{{candidateId}}.jpg")
  h2.namerep {{bio.fullName}}
  p.repinfo <strong>Party</strong> {{bio.party}}
  p.repinfo <strong>Started their term </strong> {{bio.termStart}}
  p.repinfo <strong>Birthday</strong> {{bio.birthday}}
  p.repinfo <strong>Phone</strong> {{bio.phone}}
  p.repinfo <strong>Website</strong> {{bio.website}}
  p.repinfo <strong>Contact Form</strong> {{bio.contactForm}}
  p.repinfo <strong>Fax</strong> {{bio.fax}}
  p.repinfo <strong>VoteSmart ID</strong> {{bio.voteSmartId}}
  p.repinfo <strong>Candidate ID</strong> {{bio.uniqueId}}

